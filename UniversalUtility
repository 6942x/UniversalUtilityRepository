local function UpdateJumpSlider(percentage)
    TweenService:Create(JumpSliderFill, TweenInfo.new(0.2), {Size = UDim2.new(percentage, 0, 1, 0)}):Play()
    JumpDelay = 5 + (percentage * 25)
    JumpDelayBox.Text = string.format("%.1f", JumpDelay)
    JumpDelayLabel.Text = "Jump Delay: " .. string.format("%.1f", JumpDelay) .. "s"
end

local function UpdateClickSlider(percentage)
    TweenService:Create(ClickSliderFill, TweenInfo.new(0.2), {Size = UDim2.new(percentage, 0, 1, 0)}):Play()
    ClickDelay = 1 + (percentage * 9)
    ClickDelayBox.Text = string.format("%.1f", ClickDelay)
    ClickDelayLabel.Text = "Click Delay: " .. string.format("%.1f", ClickDelay) .. "s"
end

local function UpdateSpamSlider(percentage)
    TweenService:Create(SpamSliderFill, TweenInfo.new(0.2), {Size = UDim2.new(percentage, 0, 1, 0)}):Play()
    SpamDelay = 0.05 + (percentage * 4.95)
    SpamDelayBox.Text = string.format("%.2f", SpamDelay)
    SpamDelayLabel.Text = "Delay: " .. string.format("%.2f", SpamDelay) .. "s"
end

local function UpdateFPSSlider(percentage)
    TweenService:Create(FPSFill, TweenInfo.new(0.2), {Size = UDim2.new(percentage, 0, 1, 0)}):Play()
    TargetFPS = math.floor(15 + (percentage * 345))
    FPSValueBox.Text = tostring(TargetFPS)
    FPSValueLabel.Text = "Target FPS: " .. TargetFPS
    FPSUnlockStatus.Text = "Target: " .. TargetFPS .. " FPS"
    
    if FPSUnlockEnabled then
        if setfpscap then
            setfpscap(TargetFPS)
        end
    end
end

JumpDelayBox.FocusLost:Connect(function(enterPressed)
    local value = tonumber(JumpDelayBox.Text)
    if value then
        value = math.clamp(value, 5, 30)
        JumpDelayBox.Text = string.format("%.1f", value)
        JumpDelay = value
        UpdateJumpSlider((value - 5) / 25)
    else
        JumpDelayBox.Text = string.format("%.1f", JumpDelay)
    end
end)

ClickDelayBox.FocusLost:Connect(function(enterPressed)
    local value = tonumber(ClickDelayBox.Text)
    if value then
        value = math.clamp(value, 1, 10)
        ClickDelayBox.Text = string.format("%.1f", value)
        ClickDelay = value
        UpdateClickSlider((value - 1) / 9)
    else
        ClickDelayBox.Text = string.format("%.1f", ClickDelay)
    end
end)

SpamDelayBox.FocusLost:Connect(function(enterPressed)
    local value = tonumber(SpamDelayBox.Text)
    if value then
        value = math.clamp(value, 0.05, 5)
        SpamDelayBox.Text = string.format("%.2f", value)
        SpamDelay = value
        UpdateSpamSlider((value - 0.05) / 4.95)
    else
        SpamDelayBox.Text = string.format("%.2f", SpamDelay)
    end
end)

FPSValueBox.FocusLost:Connect(function(enterPressed)
    local value = tonumber(FPSValueBox.Text)
    if value then
        value = math.clamp(value, 15, 360)
        FPSValueBox.Text = tostring(value)
        TargetFPS = value
        UpdateFPSSlider((value - 15) / 345)
    else
        FPSValueBox.Text = tostring(TargetFPS)
    end
end)

local jumpDragging, clickDragging, spamDragging, fpsDragging = false, false, false, false

JumpSliderButton.MouseButton1Down:Connect(function() jumpDragging = true end)
ClickSliderButton.MouseButton1Down:Connect(function() clickDragging = true end)
SpamSliderButton.MouseButton1Down:Connect(function() spamDragging = true end)
FPSButton.MouseButton1Down:Connect(function() fpsDragging = true end)

UserInputService.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        jumpDragging, clickDragging, spamDragging, fpsDragging = false, false, false, false
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement then
        local mouseX = UserInputService:GetMouseLocation().X
        
        if jumpDragging then
            local percentage = math.clamp((mouseX - JumpDelaySlider.AbsolutePosition.X) / JumpDelaySlider.AbsoluteSize.X, 0, 1)
            UpdateJumpSlider(percentage)
        end
        
        if clickDragging then
            local percentage = math.clamp((mouseX - ClickDelaySlider.AbsolutePosition.X) / ClickDelaySlider.AbsoluteSize.X, 0, 1)
            UpdateClickSlider(percentage)
        end
        
        if spamDragging then
            local percentage = math.clamp((mouseX - SpamDelaySlider.AbsolutePosition.X) / SpamDelaySlider.AbsoluteSize.X, 0, 1)
            UpdateSpamSlider(percentage)
        end
        
        if fpsDragging then
            local percentage = math.clamp((mouseX - FPSSlider.AbsolutePosition.X) / FPSSlider.AbsoluteSize.X, 0, 1)
            UpdateFPSSlider(percentage)
        end
    end
end)

local function UpdateAntiAFKStatus()
    if JumpEnabled and ClickEnabled then
        AntiAFKStatus.Text = "Status: Jump & Click Active"
        AntiAFKStatus.TextColor3 = Color3.fromRGB(50, 220, 100)
    elseif JumpEnabled then
        AntiAFKStatus.Text = "Status: Jump Active"
        AntiAFKStatus.TextColor3 = Color3.fromRGB(100, 200, 255)
    elseif ClickEnabled then
        AntiAFKStatus.Text = "Status: Click Active"
        AntiAFKStatus.TextColor3 = Color3.fromRGB(255, 200, 100)
    else
        AntiAFKStatus.Text = "Status: All Inactive"
        AntiAFKStatus.TextColor3 = Color3.fromRGB(180, 180, 180)
    end
end

local function UpdateClickTypeButtons()
    TweenService:Create(ClickTypeCurrent, TweenInfo.new(0.2), {
        BackgroundColor3 = (CurrentClickType == "Current") and Color3.fromRGB(100, 150, 255) or Color3.fromRGB(45, 45, 52)
    }):Play()
    TweenService:Create(ClickTypeCenter, TweenInfo.new(0.2), {
        BackgroundColor3 = (CurrentClickType == "Center") and Color3.fromRGB(100, 150, 255) or Color3.fromRGB(45, 45, 52)
    }):Play()
    TweenService:Create(ClickTypeRandom, TweenInfo.new(0.2), {
        BackgroundColor3 = (CurrentClickType == "Random") and Color3.fromRGB(100, 150, 255) or Color3.fromRGB(45, 45, 52)
    }):Play()
end

local function ToggleFPSUnlock()
    FPSUnlockEnabled = not FPSUnlockEnabled
    
    if FPSUnlockEnabled then
        TweenService:Create(FPSUnlockToggle, TweenInfo.new(0.3), {
            BackgroundColor3 = Color3.fromRGB(50, 220, 100)
        }):Play()
        FPSUnlockToggle.Text = "FPS Unlock: ON"
        FPSUnlockStatus.TextColor3 = Color3.fromRGB(50, 220, 100)
        if setfpscap then
            setfpscap(TargetFPS)
        end
        CreateNotification("üöÄ FPS Unlock", "Enabled: " .. TargetFPS .. " FPS", 2)
    else
        TweenService:Create(FPSUnlockToggle, TweenInfo.new(0.3), {
            BackgroundColor3 = Color3.fromRGB(220, 50, 50)
        }):Play()
        FPSUnlockToggle.Text = "FPS Unlock: OFF"
        FPSUnlockStatus.TextColor3 = Color3.fromRGB(180, 180, 180)
        if setfpscap then
            setfpscap(360)
        end
        CreateNotification("üõë FPS Unlock", "Disabled", 2)
    end
end

ClickTypeCurrent.MouseButton1Click:Connect(function()
    CurrentClickType = "Current"
    UpdateClickTypeButtons()
    CreateNotification("üñ±Ô∏è Click Type", "Set to: Current Mouse Location", 2)
end)

ClickTypeCenter.MouseButton1Click:Connect(function()
    CurrentClickType = "Center"
    UpdateClickTypeButtons()
    CreateNotification("üñ±Ô∏è Click Type", "Set to: Screen Center", 2)
end)

ClickTypeRandom.MouseButton1Click:Connect(function()
    CurrentClickType = "Random"
    UpdateClickTypeButtons()
    CreateNotification("üñ±Ô∏è Click Type", "Set to: Random Screen Spot", 2)
end)

FPSUnlockToggle.MouseButton1Click:Connect(function()
    ToggleFPSUnlock()
end)

KeybindButton.MouseButton1Click:Connect(function()
    if IsChangingKeybind then return end
    IsChangingKeybind = true
    KeybindButton.Text = "Press any key..."
    KeybindStatus.Text = "Waiting for input..."
    KeybindStatus.TextColor3 = Color3.fromRGB(255, 200, 100)
    
    local connection
    connection = UserInputService.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.Keyboard then
            CurrentKeybind = input.KeyCode
            local keyName = KeyCodeNames[input.KeyCode] or input.KeyCode.Name
            KeybindButton.Text = "Toggle Keybind: " .. keyName
            KeybindStatus.Text = "Keybind changed successfully!"
            KeybindStatus.TextColor3 = Color3.fromRGB(50, 220, 100)
            CreateNotification("‚å®Ô∏è Keybind Changed", "New keybind: " .. keyName, 2)
            task.wait(2)
            KeybindStatus.Text = "Click button above to change keybind"
            KeybindStatus.TextColor3 = Color3.fromRGB(150, 150, 150)
            IsChangingKeybind = false
            connection:Disconnect()
        end
    end)
end)

JumpToggle.MouseButton1Click:Connect(function()
    JumpEnabled = not JumpEnabled
    TweenService:Create(JumpToggle, TweenInfo.new(0.3), {
        BackgroundColor3 = JumpEnabled and Color3.fromRGB(50, 220, 100) or Color3.fromRGB(220, 50, 50)
    }):Play()
    JumpToggle.Text = "Auto Jump: " .. (JumpEnabled and "ON" or "OFF")
    
    if JumpEnabled then
        CreateNotification("ü¶ò Jump Activated", "Auto Jump is now active", 2)
        task.spawn(function()
            while JumpEnabled do
                task.wait(JumpDelay)
                if JumpEnabled and LocalPlayer.Character then
                    local humanoid = LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
                    if humanoid then humanoid.Jump = true end
                end
            end
        end)
    else
        CreateNotification("üõë Jump Deactivated", "Auto Jump has been disabled", 2)
    end
    UpdateAntiAFKStatus()
end)

ClickToggle.MouseButton1Click:Connect(function()
    ClickEnabled = not ClickEnabled
    TweenService:Create(ClickToggle, TweenInfo.new(0.3), {
        BackgroundColor3 = ClickEnabled and Color3.fromRGB(50, 220, 100) or Color3.fromRGB(220, 50, 50)
    }):Play()
    ClickToggle.Text = "Auto Click: " .. (ClickEnabled and "ON" or "OFF")
    
    if ClickEnabled then
        CreateNotification("üñ±Ô∏è Click Activated", "Auto Click is now active", 2)
        task.spawn(function()
            while ClickEnabled do
                task.wait(ClickDelay)
                if ClickEnabled then
                    if CurrentClickType == "Current" then
                        local mousePos = UserInputService:GetMouseLocation()
                        VirtualInputManager:SendMouseButtonEvent(mousePos.X, mousePos.Y, 0, true, game, 0)
                        task.wait(0.05)
                        VirtualInputManager:SendMouseButtonEvent(mousePos.X, mousePos.Y, 0, false, game, 0)
                    elseif CurrentClickType == "Center" then
                        local viewport = workspace.CurrentCamera.ViewportSize
                        VirtualInputManager:SendMouseButtonEvent(viewport.X/2, viewport.Y/2, 0, true, game, 0)
                        task.wait(0.05)
                        VirtualInputManager:SendMouseButtonEvent(viewport.X/2, viewport.Y/2, 0, false, game, 0)
                    elseif CurrentClickType == "Random" then
                        local viewport = workspace.CurrentCamera.ViewportSize
                        local randomX = math.random(100, viewport.X - 100)
                        local randomY = math.random(100, viewport.Y - 100)
                        VirtualInputManager:SendMouseButtonEvent(randomX, randomY, 0, true, game, 0)
                        task.wait(0.05)
                        VirtualInputManager:SendMouseButtonEvent(randomX, randomY, 0, false, game, 0)
                    end
                end
            end
        end)
    else
        CreateNotification("üõë Click Deactivated", "Auto Click has been disabled", 2)
    end
    UpdateAntiAFKStatus()
end)

AutoSpamToggle.MouseButton1Click:Connect(function()
    AutoSpamEnabled = not AutoSpamEnabled
    
    if AutoSpamEnabled then
        local keyText = SpamInput.Text:upper()
        local keyCode = KeyCodeMap[keyText]
        
        if not keyCode then
            AutoSpamEnabled = false
            CreateNotification("‚ùå Invalid Key", "Please enter a valid key (A-Z, 0-9, F1-F12)", 2)
            return
        end
        
        if keyCode == Enum.KeyCode.P or keyCode == Enum.KeyCode.G then
            AutoSpamEnabled = false
            CreateNotification("‚ùå Restricted Key", "This key is reserved for other functions", 2)
            return
        end
        
        TweenService:Create(AutoSpamToggle, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(50, 220, 100)}):Play()
        AutoSpamToggle.Text = "ON"
        AutoSpamStatus.Text = "Status: Spamming " .. keyText
        AutoSpamStatus.TextColor3 = Color3.fromRGB(50, 220, 100)
        CreateNotification("‚å®Ô∏è Spam Activated", "Spamming key: " .. keyText, 2)
        
        task.spawn(function()
            while AutoSpamEnabled do
                task.wait(SpamDelay)
                if AutoSpamEnabled then
                    VirtualInputManager:SendKeyEvent(true, keyCode, false, game)
                    task.wait(0.05)
                    VirtualInputManager:SendKeyEvent(false, keyCode, false, game)
                end
            end
        end)
    else
        TweenService:Create(AutoSpamToggle, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(220, 50, 50)}):Play()
        AutoSpamToggle.Text = "OFF"
        AutoSpamStatus.Text = "Status: Inactive"
        AutoSpamStatus.TextColor3 = Color3.fromRGB(180, 180, 180)
        CreateNotification("üõë Spam Deactivated", "Auto Key Spam has been disabled", 2)
    end
end)

local function ApplyPerformanceToAllObjects()
    for _, obj in pairs(workspace:GetDescendants()) do
        if obj:IsA("BasePart") then
            if not OriginalSettings.Materials[obj] then
                OriginalSettings.Materials[obj] = obj.Material
                OriginalSettings.Reflectances[obj] = obj.Reflectance
            end
            obj.Material = Enum.Material.SmoothPlastic
            obj.Reflectance = 0
        end
        
        if obj:IsA("Decal") or obj:IsA("Texture") then
            if not OriginalSettings.Decals[obj] then
                OriginalSettings.Decals[obj] = obj.Transparency
            end
            obj.Transparency = 1
        end
        
        if obj:IsA("ParticleEmitter") or obj:IsA("Trail") or obj:IsA("Beam") then
            if not OriginalSettings.Transparencies[obj] then
                OriginalSettings.Transparencies[obj] = obj.Enabled
            end
            obj.Enabled = false
        end
        
        if obj:IsA("Smoke") or obj:IsA("Fire") or obj:IsA("Sparkles") then
            if not OriginalSettings.Transparencies[obj] then
                OriginalSettings.Transparencies[obj] = obj.Enabled
            end
            obj.Enabled = false
        end
        
        if obj:IsA("BasePart") and obj.Transparency < 1 then
            if not OriginalSettings.CastShadows[obj] then
                OriginalSettings.CastShadows[obj] = obj.CastShadow
            end
            obj.CastShadow = false
        end
        
        if obj:IsA("MeshPart") then
            if not OriginalSettings.RenderFidelity[obj] then
                OriginalSettings.RenderFidelity[obj] = obj.RenderFidelity
            end
            pcall(function()
                obj.RenderFidelity = Enum.RenderFidelity.Performance
            end)
        end
        
        if obj:IsA("SurfaceGui") or obj:IsA("BillboardGui") or obj:IsA("SurfaceLight") or obj:IsA("SpotLight") or obj:IsA("PointLight") then
            if not OriginalSettings.Transparencies[obj] then
                OriginalSettings.Transparencies[obj] = obj.Enabled
            end
            obj.Enabled = false
        end
    end
end

local function ApplyLevel1()
    local Lighting = game:GetService("Lighting")
    OriginalSettings.Lighting.GlobalShadows = Lighting.GlobalShadows
    OriginalSettings.Lighting.FogEnd = Lighting.FogEnd
    Lighting.GlobalShadows = false
    Lighting.FogEnd = 9e9
    
    for _, obj in pairs(workspace:GetDescendants()) do
        if obj:IsA("Decal") or obj:IsA("Texture") then
            if not OriginalSettings.Decals[obj] then
                OriginalSettings.Decals[obj] = obj.Transparency
            end
            obj.Transparency = 1
        end
    end
    
    CurrentPerformanceLevel = 1
    PerformanceStatus.Text = "Current: Level 1 (Light)\n\nBasic performance improvements"
    PerformanceStatus.TextColor3 = Color3.fromRGB(100, 200, 255)
    CreateNotification("üöÄ Level 1 Applied", "Light performance boost", 2)
end

local function ApplyLevel2()
    ApplyLevel1()
    ApplyPerformanceToAllObjects()
    
    CurrentPerformanceLevel = 2
    PerformanceStatus.Text = "Current: Level 2 (Hard)\n\nReduced graphics quality"
    PerformanceStatus.TextColor3 = Color3.fromRGB(150, 220, 255)
    CreateNotification("üöÄ Level 2 Applied", "Hard performance boost", 2)
end

local function ApplyLevel3()
    ApplyLevel2()
    local Lighting = game:GetService("Lighting")
    
    if not OriginalSettings.Lighting.Brightness then
        OriginalSettings.Lighting.Brightness = Lighting.Brightness
        OriginalSettings.Lighting.OutdoorAmbient = Lighting.OutdoorAmbient
        OriginalSettings.Lighting.Ambient = Lighting.Ambient
    end
    
    Lighting.Brightness = 2
    Lighting.OutdoorAmbient = Color3.fromRGB(128, 128, 128)
    Lighting.Ambient = Color3.fromRGB(128, 128, 128)
    
    for _, obj in pairs(Lighting:GetChildren()) do
        if obj:IsA("PostEffect") or obj:IsA("BloomEffect") or obj:IsA("BlurEffect") or obj:IsA("ColorCorrectionEffect") or obj:IsA("SunRaysEffect") then
            if not OriginalSettings.Lighting[obj] then
                OriginalSettings.Lighting[obj] = obj.Enabled
            end
            obj.Enabled = false
        end
    end
    
    CurrentPerformanceLevel = 3
    PerformanceStatus.Text = "Current: Level 3 (High)\n\nMaximum visual reduction"
    PerformanceStatus.TextColor3 = Color3.fromRGB(255, 200, 100)
    CreateNotification("üöÄ Level 3 Applied", "High performance boost", 2)
end

local function ApplyLevel4()
    ApplyLevel3()
    
    pcall(function()
        settings().Rendering.QualityLevel = 1
        game:GetService("GraphicsQualityLevel").QualityLevel = 1
    end)
    
    local Lighting = game:GetService("Lighting")
    if not OriginalSettings.Lighting.ShadowSoftness then
        OriginalSettings.Lighting.ShadowSoftness = Lighting.ShadowSoftness
    end
    Lighting.ShadowSoftness = 0
    
    CurrentPerformanceLevel = 4
    PerformanceStatus.Text = "Current: Level 4 (Potato)\n\nExtreme performance for low-end PCs"
    PerformanceStatus.TextColor3 = Color3.fromRGB(255, 180, 80)
    CreateNotification("üöÄ Level 4 Applied", "Potato mode activated", 2)
end

local function ApplyLevel5()
    ApplyLevel4()
    
    pcall(function()
        settings().Rendering.QualityLevel = 1
        game:GetService("GraphicsQualityLevel").QualityLevel = 1
    end)
    
    for _, obj in pairs(workspace:GetDescendants()) do
        if obj:IsA("BasePart") then
            pcall(function()
                obj.TopSurface = Enum.SurfaceType.Smooth
                obj.BottomSurface = Enum.SurfaceType.Smooth
                obj.FrontSurface = Enum.SurfaceType.Smooth
                obj.BackSurface = Enum.SurfaceType.Smooth
                obj.LeftSurface = Enum.SurfaceType.Smooth
                obj.RightSurface = Enum.SurfaceType.Smooth
            end)
        end
    end
    
    for _, player in pairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and player.Character then
            for _, part in pairs(player.Character:GetDescendants()) do
                if part:IsA("BasePart") then
                    if not OriginalSettings.Materials[part] then
                        OriginalSettings.Materials[part] = part.Material
                    end
                    part.Material = Enum.Material.SmoothPlastic
                    part.Reflectance = 0
                end
            end
        end
    end
    
    CurrentPerformanceLevel = 5
    PerformanceStatus.Text = "Current: Level 5 (MAX Potato)\n\nAbsolute maximum performance"
    PerformanceStatus.TextColor3 = Color3.fromRGB(50, 220, 100)
    CreateNotification("üöÄ Level 5 Applied", "MAXIMUM Potato mode!", 2)
end

local function RevertAllChanges()
    local Lighting = game:GetService("Lighting")
    
    if OriginalSettings.Lighting.GlobalShadows ~= nil then
        Lighting.GlobalShadows = OriginalSettings.Lighting.GlobalShadows
    end
    if OriginalSettings.Lighting.FogEnd then
        Lighting.FogEnd = OriginalSettings.Lighting.FogEnd
    end
    if OriginalSettings.Lighting.Brightness then
        Lighting.Brightness = OriginalSettings.Lighting.Brightness
    end
    if OriginalSettings.Lighting.OutdoorAmbient then
        Lighting.OutdoorAmbient = OriginalSettings.Lighting.OutdoorAmbient
    end
    if OriginalSettings.Lighting.Ambient then
        Lighting.Ambient = OriginalSettings.Lighting.Ambient
    end
    if OriginalSettings.Lighting.ShadowSoftness then
        Lighting.ShadowSoftness = OriginalSettings.Lighting.ShadowSoftness
    end
    
    for obj, value in pairs(OriginalSettings.Lighting) do
        if typeof(obj) ~= "string" and typeof(obj) ~= "boolean" and typeof(obj) ~= "number" and typeof(obj) ~= "Color3" then
            pcall(function()
                if obj and obj.Parent then
                    obj.Enabled = value
                end
            end)
        end
    end
    
    for obj, material in pairs(OriginalSettings.Materials) do
        if obj and obj.Parent then
            pcall(function() obj.Material = material end)
        end
    end
    
    for obj, reflectance in pairs(OriginalSettings.Reflectances) do
        if obj and obj.Parent then
            pcall(function() obj.Reflectance = reflectance end)
        end
    end
    
    for obj, transparency in pairs(OriginalSettings.Decals) do
        if obj and obj.Parent then
            pcall(function() obj.Transparency = transparency end)
        end
    end
    
    for obj, value in pairs(OriginalSettings.Transparencies) do
        if obj and obj.Parent then
            pcall(function() obj.Enabled = value end)
        end
    end
    
    for obj, value in pairs(OriginalSettings.CastShadows) do
        if obj and obj.Parent then
            pcall(function() obj.CastShadow = value end)
        end
    end
    
    for obj, value in pairs(OriginalSettings.RenderFidelity) do
        if obj and obj.Parent then
            pcall(function() obj.RenderFidelity = value end)
        end
    end
    
    pcall(function()
        settings().Rendering.QualityLevel = Enum.QualityLevel.Automatic
        game:GetService("GraphicsQualityLevel").QualityLevel = Enum.QualityLevel.Automatic
    end)
    
    OriginalSettings = {
        Materials = {}, Reflectances = {}, Transparencies = {}, Decals = {},
        Lighting = {}, CastShadows = {}, RenderFidelity = {}
    }
    
    CurrentPerformanceLevel = 0
    PerformanceStatus.Text = "Current: None Applied\n\nLevels 1-3: Safe | Levels 4-5: May cause issues"
    PerformanceStatus.TextColor3 = Color3.fromRGB(180, 180, 180)
    CreateNotification("‚Ü©Ô∏è Changes Reverted", "All performance changes reverted", 2)
end

local function UpdatePerformanceButtons()
    local buttons = {Level1Button, Level2Button, Level3Button, Level4Button, Level5Button}
    for i, btn in ipairs(buttons) do
        TweenService:Create(btn, TweenInfo.new(0.3), {
            BackgroundColor3 = (i == CurrentPerformanceLevel) and Color3.fromRGB(100, 150, 255) or Color3.fromRGB(45, 45, 52)
        }):Play()
    end
end

Level1Button.MouseButton1Click:Connect(function()
    if CurrentPerformanceLevel == 1 then
        RevertAllChanges()
    else
        RevertAllChanges()
        task.wait(0.1)
        ApplyLevel1()
    end
    UpdatePerformanceButtons()
end)

Level2Button.MouseButton1Click:Connect(function()
    if CurrentPerformanceLevel == 2 then
        RevertAllChanges()
    else
        RevertAllChanges()
        task.wait(0.1)
        ApplyLevel2()
    end
    UpdatePerformanceButtons()
end)

Level3Button.MouseButton1Click:Connect(function()
    if CurrentPerformanceLevel == 3 then
        RevertAllChanges()
    else
        RevertAllChanges()
        task.wait(0.1)
        ApplyLevel3()
    end
    UpdatePerformanceButtons()
end)

Level4Button.MouseButton1Click:Connect(function()
    if CurrentPerformanceLevel == 4 then
        RevertAllChanges()
    else
        RevertAllChanges()
        task.wait(0.1)
        ApplyLevel4()
    end
    UpdatePerformanceButtons()
end)

Level5Button.MouseButton1Click:Connect(function()
    if CurrentPerformanceLevel == 5 then
        RevertAllChanges()
    else
        RevertAllChanges()
        task.wait(0.1)
        ApplyLevel5()
    end
    UpdatePerformanceButtons()
end)

RevertButton.MouseButton1Click:Connect(function()
    RevertAllChanges()
    UpdatePerformanceButtons()
end)

local function ToggleUI()
    MainFrame.Visible = not MainFrame.Visible
    ReopenButton.Visible = not MainFrame.Visible
    
    if MainFrame.Visible then
        ReopenPulse.Visible = false
        MainFrame.Size = UDim2.new(0, 0, 0, 0)
        MainFrame.Position = UDim2.new(0.5, 0, 0.5, 0)
        TweenService:Create(MainFrame, TweenInfo.new(0.6, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
            Size = UDim2.new(0, 500, 0, 650),
            Position = UDim2.new(0.5, -250, 0.5, -325)
        }):Play()
        TweenService:Create(ReopenButton, TweenInfo.new(0.4, Enum.EasingStyle.Back, Enum.EasingDirection.In), {
            Size = UDim2.new(0, 0, 0, 0)
        }):Play()
        CreateNotification("üëÅÔ∏è UI Visible", "Interface is now visible", 1.5)
    else
        TweenService:Create(MainFrame, TweenInfo.new(0.4, Enum.EasingStyle.Back, Enum.EasingDirection.In), {
            Size = UDim2.new(0, 0, 0, 0),
            Position = UDim2.new(0.5, 0, 0.5, 0)
        }):Play()
        task.wait(0.45)
        ReopenButton.Size = UDim2.new(0, 0, 0, 0)
        ReopenPulse.Visible = true
        TweenService:Create(ReopenButton, TweenInfo.new(0.6, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
            Size = UDim2.new(0, 60, 0, 60),
            Position = UDim2.new(0.5, -30, 0, 30)
        }):Play()
        CreateNotification("üîí UI Hidden", "Click circle or press keybind to show", 2)
    end
end

CloseButton.MouseEnter:Connect(function()
    TweenService:Create(CloseButton, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(220, 50, 50)}):Play()
end)

CloseButton.MouseLeave:Connect(function()
    TweenService:Create(CloseButton, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(45, 45, 52)}):Play()
end)

CloseButton.MouseButton1Click:Connect(function()
    ToggleUI()
end)

ReopenButton.MouseButton1Click:Connect(function()
    ToggleUI()
end)

ReopenButton.MouseEnter:Connect(function()
    TweenService:Create(ReopenButton, TweenInfo.new(0.3), {
        Size = UDim2.new(0, 70, 0, 70),
        Position = UDim2.new(0.5, -35, 0, 25)
    }):Play()
end)

ReopenButton.MouseLeave:Connect(function()
    TweenService:Create(ReopenButton, TweenInfo.new(0.3), {
        Size = UDim2.new(0, 60, 0, 60),
        Position = UDim2.new(0.5, -30, 0, 30)
    }):Play()
end)

UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if not gameProcessed and input.KeyCode == CurrentKeybind and not IsChangingKeybind then
        ToggleUI()
    end
end)

local lastTime = tick()
local frameCount = 0
local currentFPS = 60

RunService.RenderStepped:Connect(function()
    frameCount = frameCount + 1
    local currentTime = tick()
    
    if currentTime - lastTime >= 1 then
        currentFPS = math.floor(frameCount / (currentTime - lastTime))
        frameCount = 0
        lastTime = currentTime
        FPSLabel.Text = "FPS: " .. currentFPS
    end
end)

task.spawn(function()
    pcall(function()
        local userId = LocalPlayer.UserId
        PlayerImage.Image = "rbxthumb://type=AvatarHeadShot&id=" .. userId .. "&w=420&h=420"
        
        local gameInfo = MarketplaceService:GetProductInfo(game.PlaceId)
        GameName.Text = gameInfo.Name
        
        if gameInfo.IconImageAssetId then
            GameImage.Image = "rbxthumb://type=Asset&id=" .. gameInfo.IconImageAssetId .. "&w=420&h=420"
        end
    end)
end)

MainFrame.Visible = true
ReopenButton.Visible = false

UpdateJumpSlider(0.5)
UpdateClickSlider(0.2)
UpdateSpamSlider(0.01)
UpdateFPSSlider(0.13)
UpdateClickTypeButtons()

TweenService:Create(MainFrame, TweenInfo.new(0.8, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
    Size = UDim2.new(0, 500, 0, 650),
    Position = UDim2.new(0.5, -250, 0.5, -325)
}):Play()

CreateNotification("‚ú® Script Loaded", "Universal Utility loaded successfully!\nPress G to toggle UI", 4)
