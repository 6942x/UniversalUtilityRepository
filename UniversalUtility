local FPSHistory = {}
local MAX_POINTS = 60
local FPSGraphLines = {}

for i = 1, MAX_POINTS do
    table.insert(FPSHistory, 60)
    local line = Instance.new("Frame", FPSGraphFrame)
    line.Size = UDim2.new(0, 2, 0, 1)
    line.BackgroundColor3 = Color3.fromRGB(100, 150, 255)
    line.BorderSizePixel = 0
    line.AnchorPoint = Vector2.new(0, 1)
    table.insert(FPSGraphLines, line)
end

local function UpdateFPSGraph(fps)
    table.remove(FPSHistory, 1)
    table.insert(FPSHistory, fps)
    
    local graphWidth = FPSGraphFrame.AbsoluteSize.X - 20
    local graphHeight = FPSGraphFrame.AbsoluteSize.Y - 35
    local spacing = graphWidth / MAX_POINTS
    
    local minFPS, maxFPS = math.huge, 0
    local totalFPS = 0
    
    for _, fpsVal in ipairs(FPSHistory) do
        minFPS = math.min(minFPS, fpsVal)
        maxFPS = math.max(maxFPS, fpsVal)
        totalFPS = totalFPS + fpsVal
    end
    
    local avgFPS = math.floor(totalFPS / #FPSHistory)
    
    CurrentFPSLabel.Text = "Current: " .. fps
    AvgFPSLabel.Text = "Average: " .. avgFPS
    MinMaxFPSLabel.Text = string.format("Min: %d | Max: %d", minFPS, maxFPS)
    
    local fpsRange = math.max(maxFPS - minFPS, 10)
    
    for i, line in ipairs(FPSGraphLines) do
        local fpsValue = FPSHistory[i]
        local normalizedHeight = ((fpsValue - minFPS) / fpsRange) * graphHeight
        
        line.Size = UDim2.new(0, 2, 0, math.max(normalizedHeight, 1))
        line.Position = UDim2.new(0, 10 + (i - 1) * spacing, 1, -5)
        
        local colorIntensity = fpsValue / 120
        if fpsValue >= 60 then
            line.BackgroundColor3 = Color3.fromRGB(50, 220, 100)
        elseif fpsValue >= 30 then
            line.BackgroundColor3 = Color3.fromRGB(255, 200, 100)
        else
            line.BackgroundColor3 = Color3.fromRGB(220, 50, 50)
        end
    end
end

