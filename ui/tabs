    local ClickTypeRandom = Helpers.CreateButton(Container, UDim2.new(0, 95, 0, 35), "Random")
    ClickTypeRandom.Position = UDim2.new(0.83, 0, 0, 150)
    
    local JumpDelayFrame, JumpDelaySlider, JumpSliderFill, JumpSliderButton, JumpDelayBox = Helpers.CreateSlider(Container, UDim2.new(1, -20, 0, 50), 5, 30, 10, "‚è±Ô∏è Jump Delay (seconds)")
    JumpDelayFrame.Position = UDim2.new(0, 10, 0, 205)
    
    local ClickDelayFrame, ClickDelaySlider, ClickSliderFill, ClickSliderButton, ClickDelayBox = Helpers.CreateSlider(Container, UDim2.new(1, -20, 0, 50), 1, 10, 3, "‚è±Ô∏è Click Delay (seconds)")
    ClickDelayFrame.Position = UDim2.new(0, 10, 0, 275)
    
    local Status = Instance.new("TextLabel", Container)
    Status.Size = UDim2.new(1, -20, 0, 60)
    Status.Position = UDim2.new(0, 10, 0, 335)
    Status.BackgroundTransparency = 1
    Status.Text = "Status: All Inactive"
    Status.Font = Enum.Font.GothamBold
    Status.TextSize = 16
    Status.TextColor3 = Color3.fromRGB(180, 180, 180)
    Status.TextXAlignment = Enum.TextXAlignment.Center
    
    return {
        JumpToggle = JumpToggle,
