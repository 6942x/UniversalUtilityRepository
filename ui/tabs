            Avg = AvgFPSLabel,
            MinMax = MinMaxFPSLabel
        },
        PingStats = {
            Current = CurrentPingLabel,
            Avg = AvgPingLabel,
            MinMax = MinMaxPingLabel,
            Quality = NetworkQualityLabel
        }
    }
end

