    Container.BorderSizePixel = 0
    Container.LayoutOrder = 1
    local corner = Instance.new("UICorner", Container)
    corner.CornerRadius = UDim.new(0, 12)
    
    local ContainerGlow = Instance.new("ImageLabel", Container)
    ContainerGlow.Size = UDim2.new(1, 20, 1, 20)
    ContainerGlow.Position = UDim2.new(0.5, 0, 0.5, 0)
    ContainerGlow.AnchorPoint = Vector2.new(0.5, 0.5)
    ContainerGlow.BackgroundTransparency = 1
    ContainerGlow.Image = "rbxassetid://5028857084"
    ContainerGlow.ImageColor3 = Color3.fromRGB(100, 150, 255)
    ContainerGlow.ImageTransparency = 0.9
    ContainerGlow.ZIndex = 0
    
    local Title = Instance.new("TextLabel", Container)
    Title.Size = UDim2.new(1, -20, 0, 30)
    Title.Position = UDim2.new(0, 10, 0, 10)
    Title.BackgroundTransparency = 1
    Title.Text = "üõ°Ô∏è Anti-AFK Methods"
    Title.Font = Enum.Font.GothamBold
    Title.TextSize = 18
    Title.TextColor3 = Color3.fromRGB(255, 255, 255)
    Title.TextXAlignment = Enum.TextXAlignment.Left
    
    local JumpToggle = Helpers.CreateButton(Container, UDim2.new(0, 150, 0, 42), "Auto Jump: OFF")
    JumpToggle.Position = UDim2.new(0.25, 0, 0, 55)
    
    local ClickToggle = Helpers.CreateButton(Container, UDim2.new(0, 150, 0, 42), "Auto Click: OFF")
    ClickToggle.Position = UDim2.new(0.75, 0, 0, 55)
    
    local ClickTypeLabel = Instance.new("TextLabel", Container)
    ClickTypeLabel.Size = UDim2.new(1, -20, 0, 25)
    ClickTypeLabel.Position = UDim2.new(0, 10, 0, 115)
    ClickTypeLabel.BackgroundTransparency = 1
    ClickTypeLabel.Text = "Click Type Selection"
