task.spawn(function()
    while true do
        TweenService:Create(StatusIndicator, TweenInfo.new(1, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true), {
            BackgroundTransparency = 0.3
        }):Play()
        task.wait(0.01)
    end
end)

_G.UniversalUtility.Tabs.CreateAntiAFKTab = function()
    local Container = Instance.new("Frame", AntiAFKContent)
